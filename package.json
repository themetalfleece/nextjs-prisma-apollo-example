{
  "name": "nextjs-prisma-apollo-example",
  "version": "2.38.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "scripts": {
    "dev": "next",
    "build": "yarn generate && next build",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "rimraf src/prisma/schema.prisma && prismerge -i prismerge.json && prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/index.page",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "lint": "eslint --fix --quiet lib/**/*.ts pages/**/*.ts[x]",
    "format": "prettier --loglevel silent --write lib/**/*.ts pages/**/*.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@prisma/client": "^4.11.0",
    "apollo-server-micro": "^3.12.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "next": "^13.2.3",
    "nexus": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5"
  },
  "devDependencies": {
    "@prisma-utils/prismerge": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "@types/micro-cors": "^0.1.3",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "rimraf": "^4.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
