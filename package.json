{
  "name": "nextjs-prisma-apollo-example",
  "version": "1.13.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "scripts": {
    "dev": "next",
    "build": "yarn generate && next build",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "rimraf src/prisma/schema.prisma && prismerge -i prismerge.json && prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/index.page",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "lint": "eslint --fix --quiet lib/**/*.ts pages/**/*.ts[x]",
    "format": "prettier --loglevel silent --write lib/**/*.ts pages/**/*.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@prisma/client": "^4.5.0",
    "apollo-server-micro": "^3.10.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.0",
    "graphql-tag": "^2.12.6",
    "micro": "^9.4.1",
    "micro-cors": "^0.1.1",
    "next": "^13.0.0",
    "nexus": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3"
  },
  "devDependencies": {
    "@prisma-utils/prismerge": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "typescript-eslint-parser": "^22.0.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
